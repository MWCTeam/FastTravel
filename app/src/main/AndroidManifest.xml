<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fasttravel.tia.yak.fasttravel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuUtama"
            android:label="@string/app_name" />
        <activity android:name=".account.LoginActivity" />
        <activity android:name=".travel.MapActivity" />
        <activity android:name=".travel.ListTravelActivity" />
        <activity android:name=".travel.DetailTravelActivity" />
        <activity android:name=".travel.PesanTravelActivity"></activity>
    </application>

</manifest>